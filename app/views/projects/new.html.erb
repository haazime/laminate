<div class="row">
  <div class="breadcrumbs">
    <%= link_to 'あなたのプロジェクト', projects_url -%>
    <span class="current">
      新しいプロジェクト
    </span>
  </div>
</div>
<div class="row sw-Main-lg">
  <div class="col-lg-12 mx-auto">
    <div class="fe-Background fe-Background-lg">
      <%= form_with url: '', model: @form, scope: :form do |f| -%>
        <div class="form-group">
          <%= f.label :name %>
          <%= f.text_field :name, class: 'form-control' %>
        </div>
        <div class="frm-Actions">
          <%= f.button t('.submit'), class: 'btn btn-success' %>
          <%= link_to t('navs.cancel'), projects_url, class: 'frm-Action_Cancel' %>
        </div>
      <% end -%>
    </div>
  </div>
</div>
